<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="UserMapper">
	<select id="findById" resultType="User">
		
		SELECT *
		  FROM USERS 
		 WHERE USERID = #{userid}
		 
	</select>
	
	<insert id="create">
	
		INSERT 
		  INTO USERS 
		VALUES (
				#{userId}, 
				#{password}, 
				#{name}, 
				#{email}
				)
			
	</insert>
</mapper>